<% content_for :javascript_includes do %>
	<%= javascript_include_tag "cpu_call_tree.js" %>
<% end %>
<div class="container">
	<div class="container">
		<div class="row">
			<div class="area-top clearfix">
				<div class="pull-left header">
					<h3 class="title">
						CPU
					</h3>
					<h5>
						<span>
							CPU
						</span>
					</h5>
				</div>
			</div>
		</div>

		<div class="container">
			<div class="row">
				<h2>CPU Time Profiling Graph</h2>
				<div id="cpu-call-tree"></div>
			</div>
		</div>

		<div class="container">
			<div class="box">
				<div class="box-content">
					<table class="table table-normal" id="cpu-depth-table">
						<thead>
							<tr>
								<td>Method</td>
								<td>Exclusive Time</td>
								<td>Total %</td>
							</tr>
						</thead>
						<tbody>
							<%= render 'cpus/trace_methods', object: @trace_methods %>
						</tbody>
					</table>
				</div>
			</div>
		</div>

	</div>	
</div>